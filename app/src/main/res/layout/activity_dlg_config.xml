<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.kss.kssmarketv10.dlgConfig">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:gravity="top|center_horizontal"
        android:orientation="vertical"
        android:padding="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginBottom="0dp"
            android:background="@android:color/holo_blue_dark"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="5dp">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/ivLogoApp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:contentDescription="LogoApp"
                    android:focusable="false"
                    android:src="@drawable/kalixtomarketban_logo" />

                <ImageView
                    android:id="@+id/ivExit_Conf"
                    android:layout_width="40dp"
                    android:layout_height="35dp"
                    android:layout_gravity="top|right"
                    android:background="@drawable/selector_default"
                    android:clickable="true"
                    android:src="@drawable/btn_error" />

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:src="@drawable/btn_kalixtomarketsolo" />
            </FrameLayout>

        </LinearLayout>

        <LinearLayout
            style="@style/kss_widget.FrameRedondo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:layout_weight="1"
            android:baselineAligned="false"
            android:gravity="center_vertical|center_horizontal"
            android:orientation="vertical"
            android:padding="3dp">

            <TabHost
                android:id="@+id/thConfig"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TabWidget
                        android:id="@android:id/tabs"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" />

                    <FrameLayout
                        android:id="@android:id/tabcontent"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:id="@+id/tbEmpresa_Conf"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <GridLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:columnCount="4"
                                android:padding="5dp"
                                android:rowCount="10">

                                <FrameLayout
                                    android:id="@+id/fraFoto"
                                    android:layout_width="100dp"
                                    android:layout_height="100dp"
                                    android:layout_column="0"
                                    android:layout_row="0"
                                    android:layout_rowSpan="4"
                                    android:background="@drawable/ksslayer_redondo_gris">

                                    <ImageButton
                                        android:id="@+id/ivTakeImage_Conf"
                                        style="@style/kss_widget.FrameRedondo"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_column="0"
                                        android:layout_columnSpan="2"
                                        android:layout_gravity="bottom|right"
                                        android:layout_row="0"
                                        android:layout_rowSpan="2"
                                        android:background="@drawable/selector_default"
                                        android:clickable="true"
                                        android:drawableTop="@drawable/kss_camara"
                                        android:longClickable="true"
                                        android:padding="5dp"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/kss_camara"
                                        android:text="\nTomar Foto" />

                                    <TextView
                                        android:id="@+id/tvZoom_Conf"
                                        android:layout_width="55dp"
                                        android:layout_height="30dp"
                                        android:layout_column="0"
                                        android:layout_gravity="left|top"
                                        android:layout_row="0"
                                        android:layout_weight="0"
                                        android:alpha=".6"
                                        android:autoText="false"
                                        android:background="@drawable/selector_itempedido_estado"
                                        android:clickable="true"
                                        android:padding="0dp"
                                        android:text="Zoom"
                                        android:textAppearance="@style/Kss_TextAparence.labelRegistro"
                                        android:textSize="10pt" />

                                </FrameLayout>

                                <EditText
                                    android:id="@+id/etCIRIF_Conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="1"
                                    android:layout_gravity="bottom|left"
                                    android:layout_marginTop="10dp"
                                    android:layout_row="0"
                                    android:ems="10"
                                    android:hint="CI o RIF"
                                    android:inputType="textPersonName"
                                    android:textAllCaps="true"
                                    android:textAppearance="@style/Kss_TextAparence.txtDatos"
                                    android:textColor="@color/color_Negro"
                                    android:textColorHint="@color/color_Gris" />

                                <EditText
                                    android:id="@+id/etNombre_Conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="1"
                                    android:layout_gravity="left|center_vertical"
                                    android:layout_marginTop="10dp"
                                    android:layout_row="1"
                                    android:ems="10"
                                    android:hint="Nombre"
                                    android:inputType="textCapCharacters|textPersonName"
                                    android:textAllCaps="true"
                                    android:textAppearance="@style/Kss_TextAparence.txtDatos"
                                    android:textColor="@color/color_Negro"
                                    android:textColorHint="@color/color_Gris"
                                    android:visibility="visible" />

                                <EditText
                                    android:id="@+id/etContacto_Conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="1"
                                    android:layout_gravity="left"
                                    android:layout_row="2"
                                    android:ems="10"
                                    android:hint="Persona Contacto"
                                    android:inputType="textCapWords|textPersonName"
                                    android:textAppearance="@style/Kss_TextAparence.txtDatos"
                                    android:textColor="@color/color_Negro"
                                    android:textColorHint="@color/color_Gris" />

                                <TextView
                                    android:id="@+id/tvRIF_conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="50dp"
                                    android:layout_column="1"
                                    android:layout_gravity="left|top"
                                    android:layout_row="0"
                                    android:clickable="true"
                                    android:longClickable="true"
                                    android:text="@string/kssci_rif"
                                    android:textAppearance="@style/Kss_TextAparence.labelRegistro" />

                                <TextView
                                    android:id="@+id/tvEmpresa_conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="50dp"
                                    android:layout_column="1"
                                    android:layout_gravity="left|top"
                                    android:layout_row="1"
                                    android:clickable="true"
                                    android:longClickable="true"
                                    android:text="@string/kssEmpresa"
                                    android:textAppearance="@style/Kss_TextAparence.labelRegistro" />

                                <TextView
                                    android:id="@+id/textView33"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="0"
                                    android:layout_row="2"
                                    android:paddingRight="10dp"
                                    android:text="@string/ksscontacto"
                                    android:textAppearance="@style/Kss_TextAparence.labelRegistro" />

                                <TextView
                                    android:id="@+id/textView34"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="0"
                                    android:layout_row="3"
                                    android:paddingRight="10dp"
                                    android:text="Telefonos:"
                                    android:textAppearance="@style/Kss_TextAparence.labelRegistro" />

                                <TextView
                                    android:id="@+id/textView22"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="0"
                                    android:layout_row="4"
                                    android:paddingRight="10dp"
                                    android:text="Dirección:"
                                    android:textAppearance="@style/Kss_TextAparence.labelRegistro" />

                                <EditText
                                    android:id="@+id/etTelf_Conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="1"
                                    android:layout_gravity="left"
                                    android:layout_row="3"
                                    android:ems="10"
                                    android:hint="Números Telefónicos"
                                    android:inputType="textPhonetic|number"
                                    android:textAppearance="@style/Kss_TextAparence.txtDatos"
                                    android:textColor="@color/color_Negro"
                                    android:textColorHint="@color/color_Gris" />

                                <EditText
                                    android:id="@+id/tvDireccion_Conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="1"
                                    android:layout_gravity="left"
                                    android:layout_row="4"
                                    android:ems="10"
                                    android:hint="Dirección"
                                    android:inputType="textPersonName"
                                    android:textAppearance="@style/Kss_TextAparence.txtDatos"
                                    android:textColor="@color/color_Negro"
                                    android:textColorHint="@color/color_Gris" />

                                <TextView
                                    android:id="@+id/tvBaseD_Conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="0"
                                    android:layout_row="5"
                                    android:longClickable="true"
                                    android:paddingRight="10dp"
                                    android:text="B. Datos:"
                                    android:textAppearance="@style/Kss_TextAparence.labelRegistro"
                                    tools:ignore="HardcodedText" />

                                <EditText
                                    android:id="@+id/etURIBacxup_Conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="1"
                                    android:layout_row="6"
                                    android:ems="10"
                                    android:hint="Dirección Host"
                                    android:inputType="textPersonName"
                                    android:longClickable="false"
                                    android:textAllCaps="false"
                                    android:textAppearance="@style/Kss_TextAparence.txtDatos"
                                    android:textColor="@color/color_Negro"
                                    android:textColorHint="@color/color_Gris" />

                                <EditText
                                    android:id="@+id/etBD_Conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="1"
                                    android:layout_row="5"
                                    android:ems="10"
                                    android:hint="@string/kssbase_de_datos"
                                    android:inputType="textPersonName"
                                    android:textAllCaps="false"
                                    android:textAppearance="@style/Kss_TextAparence.txtDatos"
                                    android:textColor="@color/color_Negro"
                                    android:textColorHint="@color/color_Gris" />

                                <TextView
                                    android:id="@+id/tvURIBackup_Conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="0"
                                    android:layout_row="6"
                                    android:longClickable="true"
                                    android:paddingRight="10dp"
                                    android:text="Dir Backup: "
                                    android:textAppearance="@style/Kss_TextAparence.labelRegistro" />

                            </GridLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/tbIVA_Conf"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <GridLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:columnCount="2"
                                android:padding="5dp"
                                android:rowCount="7">

                                <FrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_column="0"
                                    android:layout_columnSpan="2"
                                    android:layout_row="4">

                                    <LinearLayout
                                        android:layout_width="100dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:layout_marginTop="10dp"
                                        android:background="@drawable/ksslayer_gris"
                                        android:clickable="true"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/tvCategorias_Conf"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center_vertical"
                                            android:background="@drawable/selector_default"
                                            android:clickable="true"
                                            android:gravity="center_vertical|center_horizontal|center"
                                            android:paddingRight="10dp"
                                            android:text="Categorías"
                                            android:textAlignment="center"
                                            android:textSize="11dp" />
                                    </LinearLayout>
                                </FrameLayout>

                                <EditText
                                    android:id="@+id/etIVA1_Conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="1"
                                    android:layout_row="0"
                                    android:ems="10"
                                    android:hint="IVA1"
                                    android:inputType="text|numberDecimal"
                                    android:maxLines="1"
                                    android:numeric="integer|decimal"
                                    android:textAppearance="@style/Kss_TextAparence.txtDatos"
                                    android:textColor="@color/color_Negro"
                                    android:textColorHint="@color/color_Gris" />

                                <EditText
                                    android:id="@+id/etIVA2_Conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="1"
                                    android:layout_row="1"
                                    android:ems="10"
                                    android:hint="IVA2"
                                    android:inputType="text|numberDecimal"
                                    android:numeric="integer|decimal"
                                    android:textAppearance="@style/Kss_TextAparence.txtDatos"
                                    android:textColor="@color/color_Negro"
                                    android:textColorHint="@color/color_Gris" />

                                <EditText
                                    android:id="@+id/etIVA3_Conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="1"
                                    android:layout_row="2"
                                    android:ems="10"
                                    android:hint="IVA3"
                                    android:inputType="text|numberDecimal"
                                    android:numeric="integer|decimal"
                                    android:textAppearance="@style/Kss_TextAparence.txtDatos"
                                    android:textColor="@color/color_Negro"
                                    android:textColorHint="@color/color_Gris" />

                                <TextView
                                    android:id="@+id/textView37"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="0"
                                    android:layout_row="0"
                                    android:paddingRight="10dp"
                                    android:text="IVA1: "
                                    android:textAppearance="@style/Kss_TextAparence.labelRegistro" />

                                <TextView
                                    android:id="@+id/textView38"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="0"
                                    android:layout_row="1"
                                    android:paddingRight="10dp"
                                    android:text="IVA2:"
                                    android:textAppearance="@style/Kss_TextAparence.labelRegistro" />

                                <TextView
                                    android:id="@+id/textView39"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="0"
                                    android:layout_row="2"
                                    android:paddingRight="10dp"
                                    android:text="IVA3:"
                                    android:textAppearance="@style/Kss_TextAparence.labelRegistro" />

                                <TextView
                                    android:id="@+id/textView40"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="0"
                                    android:layout_row="3"
                                    android:paddingRight="10dp"
                                    android:text="@string/kssganancia"
                                    android:textAppearance="@style/Kss_TextAparence.labelRegistro" />

                                <EditText
                                    android:id="@+id/etGanancia_Conf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_column="1"
                                    android:layout_row="3"
                                    android:ems="10"
                                    android:hint="Ganancia"
                                    android:inputType="text|numberDecimal"
                                    android:numeric="integer|decimal"
                                    android:textAppearance="@style/Kss_TextAparence.txtDatos"
                                    android:textColorHint="@color/color_Gris" />

                            </GridLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/tbSistema_Conf"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <GridLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:columnCount="2"
                                android:padding="5dp"
                                android:rowCount="7">

                                <LinearLayout
                                    android:id="@+id/fraGmail"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/ksslayer_azul"
                                    android:orientation="vertical"
                                    android:padding="5dp">

                                    <ImageView
                                        android:id="@+id/imageView2"
                                        android:layout_width="match_parent"
                                        android:layout_height="30dp"
                                        android:layout_column="0"
                                        android:layout_columnSpan="2"
                                        android:layout_row="5"
                                        android:background="@color/color_Smoke"
                                        android:scaleX="1.5"
                                        android:scaleY="1"
                                        android:src="@drawable/gmail5" />

                                    <GridLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="bottom">

                                        <EditText
                                            android:id="@+id/etEmail_Conf"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_column="1"
                                            android:layout_gravity="left"
                                            android:layout_row="0"
                                            android:ems="12"
                                            android:hint="Correo Electrónico"
                                            android:inputType="textEmailAddress"
                                            android:textAppearance="@style/Kss_TextAparence.txtDatos"
                                            android:textColor="@color/color_Negro"
                                            android:textColorHint="@color/color_Gris" />

                                        <EditText
                                            android:id="@+id/etPassword_Conf"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_column="1"
                                            android:layout_row="1"
                                            android:ems="12"
                                            android:hint="@string/kssPassword"
                                            android:inputType="textPassword"
                                            android:textAppearance="@style/Kss_TextAparence.txtDatos"
                                            android:textColor="@color/color_Negro"
                                            android:textColorHint="@color/color_Gris" />

                                        <TextView
                                            android:id="@+id/textView35"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_column="0"
                                            android:layout_row="0"
                                            android:paddingRight="10dp"
                                            android:text="Email:"
                                            android:textAppearance="@style/Kss_TextAparence.labelRegistro" />

                                        <TextView
                                            android:id="@+id/textView36"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_column="0"
                                            android:layout_row="1"
                                            android:paddingRight="10dp"
                                            android:text="Pass:"
                                            android:textAppearance="@style/Kss_TextAparence.labelRegistro" />
                                    </GridLayout>

                                </LinearLayout>

                                <LinearLayout
                                    android:id="@+id/fraDropBox"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_column="0"
                                    android:layout_columnSpan="2"
                                    android:layout_marginTop="5dp"
                                    android:layout_row="5"
                                    android:background="@drawable/ksslayer_azul"
                                    android:orientation="vertical"
                                    android:padding="5dp">

                                    <ImageView
                                        android:id="@+id/imageView"
                                        android:layout_width="match_parent"
                                        android:layout_height="30dp"
                                        android:layout_column="0"
                                        android:layout_columnSpan="2"
                                        android:layout_row="2"
                                        android:background="@color/color_Smoke"
                                        android:src="@drawable/dropbox_topic"/>

                                    <RelativeLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="40dp"
                                        android:layout_column="0"
                                        android:layout_gravity="center"
                                        android:layout_margin="5dp"
                                        android:background="@drawable/ksslayer_gris"
                                        android:orientation="vertical"
                                        android:padding="5dp">

                                        <TextView
                                            android:id="@+id/tvBackup_Conf"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_centerHorizontal="true"
                                            android:layout_centerVertical="true"
                                            android:background="@drawable/selector_default"
                                            android:clickable="true"
                                            android:text="Backup"
                                            android:textAppearance="@style/Kss_TextAparence.labelNormal.Blanco" />

                                    </RelativeLayout>

                                </LinearLayout>

                            </GridLayout>
                        </LinearLayout>

                    </FrameLayout>

                </LinearLayout>
            </TabHost>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_margin="0dp"
            android:background="@color/color_Smoke"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:padding="2dp"
            android:weightSum="1">

            <LinearLayout
                android:id="@+id/linearLayout2"
                style="@style/kss_widget.FrameRedondo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="false"
                android:layout_alignParentStart="false"
                android:layout_centerVertical="false"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvCancel_Conf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    android:background="@drawable/selector_default"
                    android:clickable="true"
                    android:text="Cancelar"
                    android:textAlignment="center"
                    android:textColor="@color/color_Negro" />
            </LinearLayout>

            <LinearLayout
                style="@style/kss_widget.FrameRedondo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="false"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvSave_Conf"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="4dp"
                    android:background="@drawable/selector_default"
                    android:clickable="true"
                    android:text="Guardar"
                    android:textAlignment="center"
                    android:textColor="@color/color_Negro" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_gravity="right"
            android:background="@color/colorPrimary"
            android:gravity="right"
            android:orientation="horizontal"
            android:weightSum="1"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="0dp">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.44"
                android:padding="2dp"
                android:paddingBottom="2dp"
                android:paddingLeft="5dp"
                android:paddingTop="2dp">

                <TextView
                    android:id="@+id/textView27"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal"
